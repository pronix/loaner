- title "Users"

%table.users
  %tr
    %th Name
    %th Username
    %th Email
    %th.actions Actions
  - @users.each do |user|
    %tr
      %td= user.name
      %td= user.username
      %td= user.email
      %td
        = edit_link edit_admin_user_path(user)
        = destroy_link admin_user_path(user)
